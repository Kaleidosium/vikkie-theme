  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#D1B8F3"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#AEECEF"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#FF8B91"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#FF6F6F"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#66a5e2"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#66a5e2"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#FF6F6F"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#f1e8b2"
      }
    },
    {
      "scope": "punctuation.definition.string.begin, punctuation.definition.string.end",
      "settings": {
        "foreground": "#f1e8b2"
      }
    },
    {
      "scope": "punctuation.terminator",
      "settings": {
        "foreground": "#D1B8F3"
      }
    },
    {
      "scope": "property",
      "settings": {
        "foreground": "#A66BB8"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#D1B8F3"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#f4eeff"
      }
    },
    {
      "scope": "support.class",
      "settings": {
        "foreground": "#f1e8b2"
      }
    },
    {
      "name": "String Escape",
      "scope": "constant.character.escape, text.html constant.character.entity.named, punctuation.definition.entity.html",
      "settings": {
        "foreground": "#ffe1be"
      }
    },
    {
      "name": "Boolean",
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#f1e8b2"
      }
    },
    {
      "name": "Variable Instances",
      "scope": [
        "variable.instance",
        "variable.other.instance",
        "variable.readwrite.instance",
        "variable.other.readwrite.instance",
        "variable.other.property",
        "meta.definition.variable",
        "support.constant",
        "support.variable"
      ],
      "settings": {
        "foreground": "#f1e8b2"
      }
    },
    {
      "name": "Identifier",
      "scope": "variable, support.class, entity.name.function",
      "settings": {
        "foreground": "#f4eeff"
      }
    },
    {
      "name": "Function call",
      "scope": "entity.name.function, support.function",
      "settings": {
        "foreground": "#22ced0"
      }
    },
    {
      "name": "Modules",
      "scope": "support.module, support.node",
      "settings": {
        "foreground": "#f1e8b2",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type",
      "scope": "entity.name.type, entity.other.inherited-class",
      "settings": {
        "foreground": "#f4eeff"
      }
    },
    {
      "name": "Class",
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#f1e8b2",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Class variable",
      "scope": "variable.object.property, meta.field.declaration entity.name.function",
      "settings": {
        "foreground": "#22ced0"
      }
    },
    {
      "name": "Class method",
      "scope": "meta.definition.method entity.name.function",
      "settings": {
        "foreground": "#f1e8b2"
      }
    },
    {
      "name": "Function definition",
      "scope": "meta.function entity.name.function",
      "settings": {
        "foreground": "#22ced0"
      }
    },
    {
      "name": "Template expression",
      "scope": "template.expression.begin, template.expression.end, punctuation.definition.template-expression.begin, punctuation.definition.template-expression.end",
      "settings": {
        "foreground": "#f1e8b2"
      }
    },
    {
      "name": "Reset embedded/template expression colors",
      "scope": "meta.embedded, source.groovy.embedded, meta.template.expression",
      "settings": {
        "foreground": "#f4eeff"
      }
    },
    {
      "name": "YAML key",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#f1e8b2"
      }
    },
    {
      "name": "JSON key",
      "scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
      "settings": {
        "foreground": "#f1e8b2"
      }
    },
    {
      "name": "JSON constant",
      "scope": "constant.language.json",
      "settings": {
        "foreground": "#f1e8b2"
      }
    },
    {
      "name": "JSON property",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#d8dee9"
      }
    },
    {
      "name": "CSS class",
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#f1e8b2"
      }
    },
    {
      "name": "CSS ID",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#FF6F6F"
      }
    },
    {
      "name": "CSS tag",
      "scope": "source.css entity.name.tag",
      "settings": {
        "foreground": "#66a5e2"
      }
    },
    {
      "name": "HTML tag outer",
      "scope": "meta.tag, punctuation.definition.tag",
      "settings": {
        "foreground": "#f4eeff"
      }
    },
    {
      "name": "HTML tag inner",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#f1e8b2"
      }
    },
    {
      "name": "HTML tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#66a5e2"
      }
    },
    {
      "name": "Markdown heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#f1e8b2"
      }
    },
    {
      "name": "Markdown link text",
      "scope": "text.html.markdown meta.link.inline, meta.link.reference",
      "settings": {
        "foreground": "#f1e8b2"
      }
    },
    {
      "name": "Markdown list item",
      "scope": "text.html.markdown beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#f1e8b2"
      }
    },
    {
      "name": "Markdown italic",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#f1e8b2",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#f1e8b2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold italic",
      "scope": "markup.bold markup.italic, markup.italic markup.bold",
      "settings": {
        "foreground": "#f1e8b2",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Markdown code block",
      "scope": "markup.fenced_code.block.markdown punctuation.definition.markdown",
      "settings": {
        "foreground": "#FF6F6F"
      }
    },
    {
      "name": "Markdown inline code",
      "scope": "markup.inline.raw.string.markdown",
      "settings": {
        "foreground": "#FF6F6F"
      }
    },
    {
      "name": "INI property name",
      "scope": "keyword.other.definition.ini",
      "settings": {
        "foreground": "#f1e8b2"
      }
    },
    {
      "name": "INI section title",
      "scope": "entity.name.section.group-title.ini",
      "settings": {
        "foreground": "#f1e8b2"
      }
    },
    {
      "name": "C# class",
      "scope": "source.cs meta.class.identifier storage.type",
      "settings": {
        "foreground": "#22ced0",
        "fontStyle": "underline"
      }
    },
    {
      "name": "C# class method",
      "scope": "source.cs meta.method.identifier entity.name.function",
      "settings": {
        "foreground": "#22ced0"
      }
    },
    {
      "name": "C# function call",
      "scope": "source.cs meta.method-call meta.method, source.cs entity.name.function",
      "settings": {
        "foreground": "#AEECEF"
      }
    },
    {
      "name": "C# type",
      "scope": "source.cs storage.type",
      "settings": {
        "foreground": "#22ced0"
      }
    },
    {
      "name": "C# return type",
      "scope": "source.cs meta.method.return-type",
      "settings": {
        "foreground": "#22ced0"
      }
    },
    {
      "name": "C# namespace",
      "scope": "source.cs entity.name.type.namespace",
      "settings": {
        "foreground": "#E1E1E6"
      }
    }
  ]
}
